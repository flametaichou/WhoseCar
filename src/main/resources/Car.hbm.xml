<hibernate-mapping>
    <class name = "auto.logic.Car" table = "cars">
        <id column = "car_id" name = "id" type = "java.lang.Integer" >
            <generator class = "increment"/>
        </id>

        <property column = "number" name = "number" type = "java.lang.String"/>
        <property column = "brand" name = "brand" type = "java.lang.String"/>
        <property column = "color" name = "color" type = "java.lang.String"/>


    </class>
</hibernate-mapping>