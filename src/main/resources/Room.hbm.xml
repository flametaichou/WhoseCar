<hibernate-mapping>
    <class name = "auto.logic.Room" table = "rooms">
    <id column = "room_id" name = "id" type = "java.lang.Integer" >
        <generator class = "increment"/>
    </id>

    <property column = "number" name = "number" type = "java.lang.Integer"/>

        <set name = "cars" lazy = "false">
            <key column = "room_id"/>
            <one-to-many class = "auto.logic.Car"/>
        </set>

    </class>
</hibernate-mapping>